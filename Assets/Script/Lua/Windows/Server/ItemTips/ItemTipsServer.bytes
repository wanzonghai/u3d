ItemTipsServer = Class("ItemTipsServer",WindowServerBase)
local this = ItemTipsServer

function this:ctor()
    this.super.ctor(self)
    self.assetPath = { Windows.GetViewPathByName(Windows.ItemTIpsView)}
end

function this:Init()
    this.super.Init(self)
	self.view = ItemTipsView.New(self.root.transform, self)
end

function this:SetParam(...)
    self.type = ...
end