AccountFailServer = Class("AccountFailServer",WindowServerBase)
local this = AccountFailServer

function this:ctor()
    this.super.ctor(self)
    self.assetPath = { Windows.GetViewPathByName(Windows.AccountFailView)}
end

function this:Init()
    this.super.Init(self)
	self.view = AccountFailView.New(self.root.transform, self)
end