SettingServer = Class("SettingServer", WindowServerBase)
local this = SettingServer

function this:ctor()
	this.super.ctor(self)
	self.assetPath = { Windows.GetViewPathByName(Windows.SettingView) }
end

function this:Init()
	this.super.Init(self)
	self.view = SettingView.New(self.root.transform, self)
end